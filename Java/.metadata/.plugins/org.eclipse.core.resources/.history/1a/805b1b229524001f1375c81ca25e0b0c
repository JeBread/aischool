package 일차원배열;

import java.util.Scanner;

public class Ex08배열 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int[] ans = {1, 4, 3, 2, 1};
		int[] my_ans = new int [5];
		
		for (int i = 0; i < 5; i++) {
			System.out.print(i+1 + "번답 >>");
			my_ans[i] = sc.nextInt();
		}
		
		

	}

}
