package for문;

public class Ex01for문 {

	public static void main(String[] args) {
		
		
		for (int i = 21; i <= 57; i++) {
			System.out.print(i+" ");
		}
		System.out.println(" ");
		
		for (int i = 96; i >= 53; i--) {
			System.out.print(i+" ");
		}
		
		System.out.println(" ");
		
		for (int j = 21; j <= 57; j++) {
			if (j % 2 == 1) {
				System.out.print(j+" ");
			}
			continue;
		}
	}

}
