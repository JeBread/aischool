package 월급관리프로그램;

public class TempEmployee {
	
	String empno;
	String name;
	int pay;
	public TempEmployee(String empno, String name, int pay) {
//		super();
		this.empno = empno;
		this.name = name;
		this.pay = pay;
	}
	
	// 비정규직 직원의 월급여를 결과값으로 돌려주는 메서드
	public int getMonyPay() {
		return pay/12;
	}
	
	// 사원의 정보
	public String print() {
		return empno + ":" + name + ":" + pay;
	}
	
	
	
}
