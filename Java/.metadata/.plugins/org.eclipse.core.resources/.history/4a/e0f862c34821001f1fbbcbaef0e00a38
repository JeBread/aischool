package while문;

import java.util.Scanner;

public class Ex06예제 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		System.out.print("정수 입력 : ");
		int now = sc.nextInt();
		int goal = sc.nextInt();
		int  i = 0;
		
		while (now != goal) {
			System.out.print("정수 입력 : ");
		}
		
		

	}

}
