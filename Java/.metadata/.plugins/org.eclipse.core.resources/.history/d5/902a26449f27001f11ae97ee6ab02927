package 메서드;

public class Ex08완전수구하기 {
	
	public static boolean isDivisor(int num1, int num2) {
//		if (num1 % num2 == 0) {
//			return true;
//		} else {
//			return false;
//		}
		return num1 % num2 == 0 ? true : false; // 삼항연산자 가능
	}
	
	public static void getDivisor(int a) {
		System.out.print(a + "의 약수 : ");
		for (int i = 1; i <= a; i++) {
			if (a % i == 0) {
				System.out.print(i + " ");
			}		
		}
		System.out.println();
	}
	
	public static int asddd(int a) {
		return 0;
	}

	public static void main(String[] args) {
		
		// 여기에 앞으로 나오는 모든 메서드 문제 풀기
		// 1) isDivisor() : 약수인지 아닌지 확인해주는 메서드
		int num1 = 10;
		int num2 = 2;
		boolean divisor = isDivisor(num1, num2);
		System.out.println(divisor);
		
		// 2) getDivisor() : 
		getDivisor(10);
		getDivisor(16);
		getDivisor(24);
		
		// 3) 

	}
	
	

}
