package 이항연산자;

public class Ex04논리연산자 {

	public static void main(String[] args) {
		
		// NOT (!)
		// - 부정 연산자
		System.out.println(!true);
		System.out.println(!false);
		
		// AND (&&)
		// 피연산자 항에 boolean 자료형이 와야함.
		// 양쪽다 true 면 결과는 true
		// 하나라도 false 있으면 결과는 false
		int num1 = 3;
		int num2 = 4;
		
		
		// OR (||)
		
		

	}

}
