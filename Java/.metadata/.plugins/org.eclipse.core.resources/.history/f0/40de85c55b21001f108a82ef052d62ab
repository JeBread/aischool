package while문;

import java.util.Random;

public class Ex09예제 {

	public static void main(String[] args) {
		
		// 랜덤으로 숫자를 가져와주는 기능
		Random rd = new Random();
		
		// bound(10) -> 0 ~ 9
		int num1 = rd.nextInt(1, 2);
		System.out.println(num1);
		int num2 = rd.nextInt(1, 2);
		System.out.println(num2);
		int ans = 0;
		
		
		
		

		
	}

}
